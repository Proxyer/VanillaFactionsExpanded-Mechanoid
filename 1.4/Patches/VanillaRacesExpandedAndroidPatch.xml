<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Races Expanded - Android</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="VFEM_MedicinalCompoundingFactory_MedicineGranulator" or defName="VFEM_VersatileAssembler"]/comps</xpath>
					<value>
						<li Class="PipeSystem.CompProperties_RefillWithPipes">
							<pipeNet>VREA_NeutroamineNet</pipeNet>
							<thing>Neutroamine</thing>
						</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>